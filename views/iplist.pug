//- IP List 
extends master.pug 

block content 
    table(id="example" class="table table-striped table-bordered" cellspacing="0" width="100%")
        thead
            tr
                th IP 
                th Max Count 
        tfoot
            tr
                th IP
                th Max Count 
        tbody
            each dict in ip_list
                tr
                    td= dict['ip']
                    td= dict['count']
        //- each dict in ip_list
            li= dict['ip'] + " --- " + dict['count']